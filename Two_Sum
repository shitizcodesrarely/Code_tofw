//i am writing a code using brute force method of Two sum in java which is a easy level question of java
package com.java.practise;

public class twosum {
	public static void main(String[]args) {
		int[] nums = {1,2,3,4,5};
		int target = 7;
		
		for(int i = 0; i<nums.length-1;i++) {
			for(int j = i+1;j<nums.length;j++) {
				if(nums[i] + nums[j] == target) {
					System.out.println("indexs are :" + i + "," + j);
				}
			}
		}
	}


}
